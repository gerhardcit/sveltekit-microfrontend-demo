{
	"name": "module-federation-vite-sveltekit-host",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "NODE_OPTIONS=--experimental-vm-modules vite dev --port 5173",
		"build": "vite build",
		"preview": "NODE_OPTIONS=--experimental-vm-modules vite preview --port 5173",
